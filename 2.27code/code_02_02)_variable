# In this script, we will practice how to use variables

room = "C226"
print("My classroom is CBPM, room", room)

building = "CBPM"
room = "C226"
usage = "classroom"
print(f"Hello, my", usage, "is", building, room)

building = input("Enter your building: ")
room = input("enter your room number: ")
usage = input("Enter your romm's usage: ")
print("Hello, my", usage, "is", building, room)